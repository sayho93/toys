{
  "name": "toys",
  "version": "1.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "start:dev": "nodemon src/app.js",
    "start": "node src/app.js"
  },
  "imports": {
    "#src/*": "./src/*.js",
    "#bin/*": "./src/bin/*.js",
    "#configs/*": "./src/configs/*.js",
    "#datasources/*": "./src/datasources/*.js",
    "#models/*": "./src/models/*.js",
    "#repositories/*": "./src/repositories/*.js",
    "#routes/*": "./src/routes/*.js",
    "#controllers/*": "./src/controllers/*.js",
    "#services/*": "./src/services/*.js",
    "#utils/*": "./src/utils/*.js",
    "#tests/*": "./tests/*.js"
  },
  "jest": {
    "transform": {},
    "rootDir": "./",
    "moduleNameMapper": {
      "#src/(.*)$": "<rootDir>/src/$1",
      "#bin/(.*)$": "<rootDir>/src/bin/$1",
      "#configs/(.*)$": "<rootDir>/src/configs/$1",
      "#datasources/(.*)$": "<rootDir>/src/datasources/$1",
      "#models/(.*)$": "<rootDir>/src/models/$1",
      "#repositories/(.*)$": "<rootDir>/src/repositories/$1",
      "#routes/(.*)$": "<rootDir>/src/routes/$1",
      "#controllers/(.*)$": "<rootDir>/src/controllers/$1",
      "#services/(.*)$": "<rootDir>/src/services/$1",
      "#utils/(.*)$": "<rootDir>/src/utils/$1",
      "#tests/(.*)$": "<rootDir>/tests/$1"
    }
  },
  "dependencies": {
    "awilix": "^7.0.3",
    "axios": "^0.27.2",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.18.1",
    "express-validator": "^6.14.0",
    "firebase-admin": "^10.0.2",
    "http-errors": "^2.0.0",
    "image-size": "^1.0.1",
    "ioredis": "^5.0.4",
    "lodash": "^4.17.21",
    "moment": "^2.29.3",
    "mongoose": "^6.3.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "mysql2": "^2.3.3",
    "node-schedule": "^2.1.0",
    "nodemailer": "^6.7.5",
    "pretty-format": "^28.0.2",
    "qs": "^6.10.3",
    "slash": "^4.0.0",
    "socket.io": "^4.5.0",
    "spdy": "^4.0.2",
    "underscore": "^1.13.3",
    "winston": "^3.7.2",
    "winston-daily-rotate-file": "^4.6.1"
  },
  "devDependencies": {
    "jest": "^28.0.3",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "supertest": "^6.2.3"
  },
  "nodemonConfig": {
    "ignore": [
      "*.json",
      "*.html",
      "*.test.js"
    ]
  }
}
